            <filters>
              <scriptletFilter id="b932efc4-9cdf-4c17-9843-3935d71d7ea7">
                <annotation></annotation>
                <name>Scriptlet</name>
                <scriptlet id="ef34f70a-818f-4aaf-aea2-7c2a927667b6">
                  <annotation></annotation>
                  <name>default</name>
                  <script>var myXMLFilename = azureXMLFileName
var myXMLocation = xmlLocation

var myOutput = &apos;&apos;;

var myDosDelimiter = &quot;\\&quot;;
var myLinuxDelimiter = &quot;/&quot;;

var endStr = myXMLocation.substring(myXMLocation.length -1, myXMLocation.length);

switch (endStr) {

	case myDosDelimiter:
		myOutput = myXMLocation  + myXMLFilename;
		break;
	case myLinuxDelimiter:
		myOutput = myXMLocation  + myXMLFilename;
		break;
	default:
		if (myXMLocation.indexOf(&quot;:&quot;) &gt; 0) {
			myOutput = myXMLocation + myDosDelimiter + myXMLFilename;
		}
		else {
			myOutput = myXMLocation + myLinuxDelimiter + myXMLFilename;		
		}	
		
}

scriptletResult = myOutput;
scriptletResponse = &quot;success&quot;;</script>
